<div class="pagination">
	<%= will_paginate @employee %>
</div> 
<table border="1", class="table">
	<tr>
		<th>Name</th>
		<th>Phone</th>
		<th>Salary</th>
		<th>Email</th>
		<!-- <th>Action</th> -->
	</tr>
	
	<% @employee.each do |emp| %>
	<tr>
		<td><%= emp.name %></td>
		<td><%= emp.phone %></td>
		<td><%= emp.salary %></td>
		<td><%= emp.email %></td>
		<!-- <td><%= link_to "Send", "#" %></td> -->
	</tr>
	<% end %>
</table>

<%#= debug @employee %>
